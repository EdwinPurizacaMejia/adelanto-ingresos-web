{
  "name": "adelanto-ingresos-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:adelanto_ingresos_web": "node dist/adelanto_ingresos_web/server/server.mjs",
    "build:github": "ng build --configuration github-pages",
    "predeploy": "npm run build:github",
    "deploy": "npx angular-cli-ghpages --dir=dist/adelanto_ingresos_web/browser",
    "deploy:quick": "npm run build:github && npx angular-cli-ghpages --dir=dist/adelanto_ingresos_web/browser",
    "deploy:force": "ng build --configuration github-pages && npx angular-cli-ghpages --dir=dist/adelanto_ingresos_web/browser --repo=https://github.com/TU_USUARIO/adelanto-ingresos-web.git --branch=gh-pages --name=\"Tu Nombre\" --email=tu@email.com",
    "deploy:all": "npm run build:github && npx angular-cli-ghpages --dir=dist/adelanto_ingresos_web/browser --no-silent"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^20.2.12",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^20.2.12",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.17",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.26.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}